[
  {
    "context": "Workspace",
    "bindings": {
      "cmd-1": "workspace::ToggleLeftDock"
    }
  },
  {
    "context": "Pane",
    "bindings": {
      "ctrl-shift-h": "pane::ActivatePrevItem",
      "ctrl-shift-l": "pane::ActivateNextItem"
    }
  },
  {
    "context": "Terminal",
    "bindings": {
      "ctrl-n": ["terminal::SendKeystroke", "down"],
      "ctrl-p": ["terminal::SendKeystroke", "up"]
    }
  },
  {
    "context": "vim_mode == normal",
    "bindings": {
      "s": ["vim::PushSneak", {}],
      "S": ["vim::PushSneakBackward", {}],
      "= =": "editor::Format",
      "[ g": "editor::GoToPrevHunk",
      "] g": "editor::GoToHunk",
      "g i": "editor::GoToImplementation",
      "space g a": "editor::ToggleGitBlame",
      "space g r": "editor::RevertSelectedHunks"
    }
  },
  {
    "context": "vim_mode == visual",
    "bindings": {
      "s": ["vim::PushSneak", {}],
      "S": ["vim::PushSneakBackward", {}],
      "= =": "editor::FormatSelections",
      "space g r": "editor::RevertSelectedHunks"
    }
  }
]
